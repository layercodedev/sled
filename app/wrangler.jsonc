{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "coder",
  "main": "src/index.tsx",
  "compatibility_date": "2025-09-19",
  "observability": {
    "enabled": true,
    // "head_sampling_rate": 1
  },
  "durable_objects": {
    "bindings": [
      {
        "class_name": "ClaudeContainer",
        "name": "CLAUDE_CONTAINER",
      },
    ],
  },
  "migrations": [
    {
      "new_sqlite_classes": ["ClaudeContainer"],
      "tag": "v1",
    },
  ],
  "assets": {
    "directory": "public",
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "coder",
      "database_id": "c3dd73bf-6dc9-4496-a5cc-7d3c54b83c25",
    },
  ],
  // External voice worker URL for deployed environments
  // In local dev, override this in .dev.vars or leave unset to use built-in voice endpoint
  "vars": {
    "VOICE_WORKER_URL": "https://api-oss.layercode.com",
  },
  // Production-only routes (use: wrangler deploy --env production)
  "env": {
    "production": {
      "routes": [
        { "pattern": "*.layercode.ai/*", "zone_name": "layercode.ai" },
        { "pattern": "layercode.ai/*", "zone_name": "layercode.ai" },
      ],
    },
  },
  // "compatibility_flags": [
